    "1650476180565e4951": {
      "type": "number-text-grid",
      "blocks": {
        "20e3799c-30c9-4487-838c-9e23d4afadb8": {
          "type": "text_block",
          "settings": {
            "title": "Keep it fresh.",
            "text": "Use recently roasted coffee, ground immediately before brewing."
          }
        },
        "d7c72e42-141d-44fe-9dbd-1fe34833553d": {
          "type": "text_block",
          "settings": {
            "title": "Not too hot!",
            "text": "Brew with water heated to between 195º to 205º Fahrenheit."
          }
        },
        "bd0afeb1-bb27-4c3c-8c01-43f2a02a81ea": {
          "type": "text_block",
          "settings": {
            "title": "Perfect Ratio.",
            "text": "Use one part coffee to 15–18 parts water. If you’re adjusting the ratio for the first time, start in the middle of this range and adjust to taste."
          }
        },
        "f292d332-80eb-49c2-8119-f42c5097c826": {
          "type": "text_block",
          "settings": {
            "title": "Storage is Key",
            "text": "Keep whole-bean coffee in an airtight container, out of direct sunlight, and away from heat and moisture."
          }
        }
      },
